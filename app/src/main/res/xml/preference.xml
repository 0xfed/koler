<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="@string/pref_reject_call_timer_default_value"
        android:entries="@array/pref_call_timer_entries"
        android:entryValues="@array/pref_call_timer_entry_values"
        android:key="@string/pref_reject_call_timer_key"
        android:title="@string/pref_reject_call_timer_title" />

    <ListPreference
        android:defaultValue="@string/pref_answer_call_timer_default_value"
        android:entries="@array/pref_call_timer_entries"
        android:entryValues="@array/pref_call_timer_entry_values"
        android:key="@string/pref_answer_call_timer_key"
        android:title="@string/pref_answer_call_timer_title" />

    <ListPreference
        android:defaultValue="@string/pref_default_page_default_value"
        android:entries="@array/pref_default_page_entries"
        android:entryValues="@array/pref_default_page_entry_values"
        android:key="@string/pref_default_page_key"
        android:title="@string/pref_default_page_title" />

    <ListPreference
        android:defaultValue="false"
        android:entries="@array/pref_is_silent_entries"
        android:entryValues="@array/pref_is_silent_entry_values"
        android:key="@string/pref_is_silent_key"
        android:title="@string/pref_is_silent_title" />

    <ListPreference
        android:defaultValue="false"
        android:entries="@array/pref_is_no_vibrate_entries"
        android:entryValues="@array/pref_is_no_vibrate_entry_values"
        android:key="@string/pref_is_no_vibrate_key"
        android:title="@string/pref_is_no_vibrate_title" />

</PreferenceScreen>